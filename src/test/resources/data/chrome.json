{
    "platform": "chrome",
    "startMaximized": true,
    "baseUrl": "https://www.google.com",
    "capabilities": {
      "browserName": "chrome",
      "seleniumProtocol": "WebDriver",
      "platform": "WIN",
      "chromeOptions": {
        "args": [
          "--start-maximized",
          "--disable-extensions",
          "--disable-infobars",
          "--disable-notifications",
          "--no-sandbox",
          "--disable-gpu"
        ]
      }
    }
}
