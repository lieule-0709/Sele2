{
  "platform": "chrome",
  "headless": true,
  "startMaximized": true,
  "baseUrl": "https://www.foody.vn/",
  "timeout": "5000",
  "PollingInterval": "500",
  "capabilities": {
    "browserName": "chrome",
    "seleniumProtocol": "WebDriver",
    "platform": "WIN",
    "chromeOptions": {
      "args": [
        "--start-maximized",
        "--disable-extensions",
        "--disable-infobars",
        "--disable-notifications",
        "--no-sandbox",
        "--disable-gpu"
      ]
    }
  }
}
